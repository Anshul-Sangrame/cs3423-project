# Functions in Tabulate #
fun addNumbers(a, b) {
    let result = ADD(a, b);
    return result;
}

fun findMax(range) {
    let maxVal = -10000;
    let rangeLength = LENGTH(range);
    let currIdx = 0;

    while(currIdx < rangeLength) {
        if(GET(range, currIdx) > maxVal) {
            maxVal = GET(range, currIdx);
        }
        currIdx = ADD(currIdx, 1);
    }
    
    return maxVal;
}


