# Compound statements in Tabulate #
let val1 = GET(cell1);
let val2 = GET(cell2);
let res;

if(val1 > 50) {
    if(val2 > 20) {
        res = ADD(MUL(val1,0.8),val2);
    } else {
        res = MUL(val1,0.9);
    }
} else {
    if(val2 < 10) {
        res = MUL(ADD(val1,val2), 1.2);
    } else {
        res = ADD(val1,val2);
    }
}
